<%= form_with url: principals_path do |f| %>
  <div>
    <%= f.label "Principal"%>
  </div>
  <div>
    <%= f.label "Name"%>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <br />
  <div>
    <%= f.label "Surname"%>
    <%= f.text_field :surname, class: 'form-control' %>
  </div>
  <br />
  <div>
    <%= f.label "E-mail"%>
    <%= f.text_field :email, class: 'form-control' %>
  </div>
  <br />
  <br /><br />
  <div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4">
      <%= f.submit :class => 'btn btn-success'%>
    </div>
  </div>
<% end %>
